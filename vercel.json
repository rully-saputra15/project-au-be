{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "app.js"
        }
    ],
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}
